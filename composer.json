{
  "name": "iserv/rector-rules",
  "description": "Provides rector rules for IServ libraries.",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "rector/rector": "~0.19.0"
  },
  "autoload": {
    "psr-4": {
      "IServ\\Library\\RectorRules\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "cotor": {
      "friendsofphp/php-cs-fixer": "^3.1",
      "vimeo/psalm": "^4.10"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.iserv.eu"
    },
    {
      "packagist": false
    }
  ],
  "homepage": "https://git.iserv.eu/iserv/lib/rector-rules"
}
